<div class="right-sidebar">
	<% if @songs %>
		<ul>
		<% @songs.each do |song|%>
			<li class="song-link"><%= song.artist_name %>: <%= song.title %></li>
		<% end %>
		</ul>
	<% end %>
	<script>
  	$('.song-link').click(function() {
	    var routeLink = $(this);
	  
	    $.getJSON( "routes/" + routeLink.data('id'), function( data );
	  }
	</script>
</div>